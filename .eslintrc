{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {
    "window": true
  },
  "rules": {
    "strict": [0],
    "eqeqeq": 2,
    "quotes": [1, "single", {"allowTemplateLiterals": true}],
    "no-underscore-dangle": 0,
    "eol-last": 0,
    "camelcase": 0,
    "no-loop-func": 0,
    "no-trailing-spaces": 1,
    "consistent-return": 0,
    "new-cap": 0,
    "no-shadow": 0,
    "no-process-exit": 0,
    "no-empty": 1,
    "yoda": 0,
    "no-cond-assign": 0,
    "no-new-func": 0,
    "no-labels": 0,
    "no-undef": 1,
    "no-unused-vars": 1,
  }
}